{
  "name": "@data-driven-forms/form-builder",
  "version": "0.0.10-rc6",
  "description": "Interactive drag and drop editor for creating data driven forms schema",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "jest --coverage",
    "start": "webpack-dev-server --hot --open --mode development",
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "BABEL_ENV=cjs rollup -c ./rollup.config.js --format=cjs --environment FORMAT:cjs",
    "build:esm": "BABEL_ENV=esm rollup -c ./rollup.config.js --format=esm --environment FORMAT:esm",
    "lint": "eslint ./src; eslint ./pf4-mappers"
  },
  "author": "Martin Marosi",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@data-driven-forms/mui-component-mapper": "^2.0.8",
    "@data-driven-forms/pf4-component-mapper": "^2.0.8",
    "@data-driven-forms/react-form-renderer": "^2.0.8",
    "@material-ui/core": "4.6.0",
    "@material-ui/icons": "^4.9.1",
    "@patternfly/react-core": "^3.134.2",
    "@patternfly/react-icons": "^3.14.39",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "prettier": "1.19.1",
    "redux-mock-store": "^1.5.4",
    "rollup": "^1.27.10",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-pluginutils": "^2.8.2",
    "style-loader": "^1.0.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "clsx": "^1.0.4",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^12.1.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5"
  },
  "peerDependencies": {
    "@data-driven-forms/mui-component-mapper": "^2.0.8",
    "@data-driven-forms/react-form-renderer": "^2.0.8",
    "@data-driven-forms/pf4-component-mapper": "^2.0.8",
    "@material-ui/core": "4.6.0",
    "@material-ui/icons": "^4.9.1",
    "@patternfly/react-core": "^3.134.2",
    "@patternfly/react-icons": "^3.14.39"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
